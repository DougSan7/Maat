<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.maat.controller.ConsultaProjetosController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Projetos Cadastrados">
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </Label>
        <TableView fx:id="tabelaProjetos" prefHeight="250.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="colunaId" prefWidth="50.0" text="ID" />
                <TableColumn fx:id="colunaNome" prefWidth="150.0" text="Nome" />
                <TableColumn fx:id="colunaDescricao" prefWidth="250.0" text="Descrição" />
                <TableColumn fx:id="colunaDataInicio" prefWidth="120.0" text="Data Início" />
                <TableColumn fx:id="colunaDataPrevistaFim" prefWidth="120.0" text="Fim Previsto" />
                <TableColumn fx:id="colunaDataFim" prefWidth="120.0" text="Data Fim" />
            </columns>
        </TableView>
        <Button onAction="#voltarParaMenu" prefWidth="100.0" text="Voltar">
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </Button>
    </children>
</VBox>